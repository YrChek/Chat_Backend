{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "no-shadow": ["error", { "allow": ["app"] }],
        "no-use-before-define": ["error", { "variables": false }]
    }
}
